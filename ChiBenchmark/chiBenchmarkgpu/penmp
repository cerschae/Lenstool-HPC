# 28 "cudafunctions.cu" 2





void cudasafe( cudaError_t error, std::string message)
{
 if(error!=cudaSuccess) {
    fprintf(stderr,"ERROR: %s : %s \n",message.c_str(),cudaGetErrorString(error));
    exit(-1);
 }
}
